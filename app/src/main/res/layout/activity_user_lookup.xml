<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.peter.diabeatdis_android.UserLookupActivity">

    <ScrollView
        android:layout_width="572dp"
        android:layout_height="143dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent">

        <TableLayout
            android:id="@+id/TableLayout_user_lookup_table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </TableLayout>
    </ScrollView>

    <AutoCompleteTextView
        android:id="@+id/ACTextView_user_lookup_location"
        style="@android:style/Widget.Holo.AutoCompleteTextView"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintStart_toStartOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <CheckBox
        android:id="@+id/checkBox_user_lookup_if_proficient"
        style="@android:style/Widget.Material.Light.CompoundButton.CheckBox"
        android:layout_width="125dp"
        android:layout_height="18dp"
        android:text="@string/check_proficient"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <CheckBox
        android:id="@+id/checkBox_user_lookup_if_limited"
        style="@android:style/Widget.Material.Light.CompoundButton.CheckBox"
        android:layout_width="125dp"
        android:layout_height="18dp"
        android:text="@string/check_limited"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_user_lookup_if_proficient" />

    <CheckBox
        android:id="@+id/checkBox_user_lookup_if_no_training"
        style="@android:style/Widget.Material.Light.CompoundButton.CheckBox"
        android:layout_width="125dp"
        android:layout_height="18dp"
        android:text="@string/check_none"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_user_lookup_if_limited" />

    <CheckBox
        android:id="@+id/checkBox_user_lookup_if_health_worker"
        style="@android:style/Widget.Material.Light.CompoundButton.CheckBox"
        android:layout_width="125dp"
        android:layout_height="18dp"
        android:text="@string/check_health_worker"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <CheckBox
        android:id="@+id/checkBox_user_lookup_if_doctor"
        style="@android:style/Widget.Material.Light.CompoundButton.CheckBox"
        android:layout_width="125dp"
        android:layout_height="18dp"
        android:text="@string/check_doctor"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_user_lookup_if_health_worker" />

    <CheckBox
        android:id="@+id/checkBox_user_lookup_if_admin"
        style="@android:style/Widget.Material.Light.CompoundButton.CheckBox"
        android:layout_width="125dp"
        android:layout_height="18dp"
        android:text="@string/check_admin"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/checkBox_user_lookup_if_doctor" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/string_training_level"
        app:layout_constraintEnd_toStartOf="@+id/textView17"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/editText_user_lookup_id" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/string_type"
        app:layout_constraintEnd_toStartOf="@+id/textView14"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/string_location"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="@string/string_enter_user_id"
        app:layout_constraintBottom_toTopOf="@+id/editText_user_lookup_id"
        app:layout_constraintEnd_toEndOf="@+id/editText_user_lookup_id"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@+id/editText_user_lookup_id" />

    <EditText
        android:id="@+id/editText_user_lookup_id"
        style="@style/Widget.AppCompat.Light.AutoCompleteTextView"
        android:layout_width="221dp"
        android:layout_height="36dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="@string/hint_identifier"
        android:inputType="textPersonName"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button40" />

    <Button
        android:id="@+id/button40"
        android:layout_width="106dp"
        android:layout_height="38dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/button_main_menu"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintEnd_toStartOf="@+id/button54"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button41"
        android:layout_width="92dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:text="@string/button_search"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText_user_lookup_id"
        app:layout_constraintStart_toEndOf="@+id/editText_user_lookup_id" />

    <Button
        android:id="@+id/button54"
        android:layout_width="92dp"
        android:layout_height="38dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/button_logout"
        android:textAppearance="@style/TextAppearance.AppCompat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner_user_lookup_location"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:entries="@array/and_or"
        android:prompt="@string/prompt_and_or"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/textView17"
        app:layout_constraintTop_toTopOf="@+id/spinner_user_lookup_training" />

    <Spinner
        android:id="@+id/spinner_user_lookup_training"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="72dp"
        android:entries="@array/and_or"
        android:prompt="@string/prompt_and_or"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/spinner_user_lookup_user_type" />

    <Spinner
        android:id="@+id/spinner_user_lookup_user_type"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginEnd="48dp"
        android:entries="@array/and_or"
        android:prompt="@string/prompt_and_or"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/textView18"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/textView18"
        app:layout_constraintVertical_bias="1.0" />

</android.support.constraint.ConstraintLayout>
